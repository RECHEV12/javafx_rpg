<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="590.0" prefWidth="885.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rpg.controller.BattleResultController">
   <children>
      <Button fx:id="goField" layoutX="16.0" layoutY="19.0" mnemonicParsing="false" onAction="#goingField" prefHeight="36.0" prefWidth="59.0" text="필드로" />
      <Button fx:id="goHome" layoutX="88.0" layoutY="19.0" mnemonicParsing="false" onAction="#goHome" prefHeight="36.0" prefWidth="59.0" text="HOME" />
      <Label fx:id="label" layoutX="249.0" layoutY="30.0" />
      <Button fx:id="goAllFied" layoutX="165.0" layoutY="19.0" mnemonicParsing="false" onAction="#goAllField" prefHeight="36.0" prefWidth="59.0" text="전체맵" />
      <AnchorPane layoutX="25.0" layoutY="90.0" prefHeight="18.0" prefWidth="352.0">
         <children>
            <ProgressBar fx:id="userExpBar" layoutX="1.0" prefHeight="18.0" prefWidth="829.0" progress="0.0" style="-fx-accent: #5ee05e;" />
            <Label fx:id="userExpLabel" alignment="CENTER" layoutX="1.0" layoutY="2.0" prefHeight="16.0" prefWidth="829.0" text="userName" />
         </children>
      </AnchorPane>
      <ScrollPane layoutX="25.0" layoutY="125.0" prefHeight="418.0" prefWidth="829.0">
         <content>
            <VBox fx:id="expLog" prefHeight="416.0" prefWidth="824.0" />
         </content>
      </ScrollPane>
      <Label layoutX="88.0" layoutY="68.0" text="EXP">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label fx:id="nowLv" layoutX="25.0" layoutY="68.0" text="Lv.">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
