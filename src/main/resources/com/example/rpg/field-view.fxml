        <Button layoutX="470.0" layoutY="451.0" mnemonicParsing="false" onAction="#goSeventhField" prefHeight="22.0" prefWidth="195.0" text="보스전 : 거대 골렘 레벨 : 30이상" />
        <Button layoutX="677.0" layoutY="451.0" mnemonicParsing="false" onAction="#goEighthField" prefHeight="22.0" prefWidth="167.0" text="보스전 : 드래곤 레벨 : 40" />
