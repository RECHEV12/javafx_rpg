        <Button layoutX="283.0" layoutY="178.0" mnemonicParsing="false" onAction="#goSecondField" text="무너진 성터 레벨 : 10이상" />
        <Button layoutX="498.0" layoutY="178.0" mnemonicParsing="false" onAction="#goThirdField" text="옛 전쟁터 레벨 : 20이상" />
